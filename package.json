{
  "name": "dominate-local-leads-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "clean:cache": "rm -rf .next tsconfig.tsbuildinfo .eslintcache",
    "clean:modules": "rm -rf node_modules",
    "clean:packages": "rm -rf package-lock.json",
    "clean:env": "echo 'Environment files (.env.local) kept for reuse. Delete manually if needed.'",
    "clean:all": "npm run clean:cache && npm run clean:modules && npm run clean:packages && npm run clean:env && echo '✅ Complete cleanup finished! All development artifacts removed.'",
    "setup": "npm install && echo '✅ Development environment ready! Run: npm run dev'",
    "reset": "npm run clean:all && npm run setup",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "lucide-react": "^0.451.0",
    "next": "14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^22.8.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  }
}
